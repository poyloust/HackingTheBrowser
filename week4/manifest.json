{
    "manifest_version":2,
    "name":"tab overdose",
    "version":"1.0",
    "icons":{
        "32":"icon32.png",
        "16":"icon16.png"
    },

    "browser_action":{
        "default_title":"TabOD",
        "default_popup":"popup.html"
    },
    "permissions":["tabs","sessions"],

    "background":{
        "scripts":["background.js"],
        "persistent":false
    }
}